{
  "name": "torii",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write \"**/*.{js,ts,tsx,mjs,mts,cjs,cts,json,css,scss,yaml,yml,webmanifest,md}\"",
    "lint": "npx eslint . && prettier --check .",
    "precommit": "lint-staged",
