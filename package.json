    "@formkit/auto-animate": "0.8.2",
    "@xstate/react": "4.1.1",
    "autoprefixer": "10.4.19",
    "classnames": "2.5.1",
    "debounce": "2.0.0",
    "formik": "2.4.5",
    "next": "14.2.2",
    "pino": "8.20.0",
    "postcss": "8.4.38",
    "react": "18.2.0",
    "react-dom": "18.2.0",
