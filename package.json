{
  "name": "torii",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "prettier --write \"**/*.{js,ts,tsx,mjs,mts,cjs,cts,json,css,scss,yaml,yml,webmanifest,md,html}\"",
    "lint": "npx eslint . && prettier --check .",
    "precommit": "lint-staged",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,scss,yaml,yml,webmanifest,mts,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-safe-important": {}
    }
  },
  "dependencies": {
    "@chakra-ui/react": "2.8.2",
    "@dicebear/collection": "8.0.1",
    "@dicebear/core": "8.0.1",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@formkit/auto-animate": "0.8.2",
    "@xstate/react": "4.1.1",
    "autoprefixer": "10.4.19",
    "classnames": "2.5.1",
    "debounce": "2.0.0",
    "formik": "2.4.6",
    "pino": "9.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.23.1",
    "react-use-websocket": "4.8.1",
    "xstate": "5.13.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "20.12.11",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.7",
    "eslint-plugin-vitest": "0.4.1",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "postcss": "8.4.38",
    "postcss-safe-important": "2.0.1",
    "prettier": "3.2.5",
    "prettier-plugin-css-order": "2.1.2",
    "sass": "1.77.1",
    "typescript": "5.4.5",
    "vite": "5.2.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  }
}
