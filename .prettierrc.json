{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "importOrder": [
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^([./]|src/)",
    "(.css|.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
